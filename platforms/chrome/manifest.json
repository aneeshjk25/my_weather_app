{
  "name": "my weather app",
  "version": "0.1",
  "description": "weather extension",
  "icons": {

  },
  "background": {
    "scripts": ["vendor/jquery/jquery-1.12.1.js", "js/browser.js", "js/services.js", "js/main.js"]
  },
  "browser_action" : {
	"default_icon": "images/icon.png",
  "default_title": "Weather",
  "default_popup": "html/index.html"
  },
  "manifest_version": 2,
  "permissions": [ "alarms", "storage", "geolocation", "*://*.openweathermap.org/*"]
}
